                            </resource>
                            <resource id="Mailjet_Mailjet::config_transactional" title="Mailjet Transaction Section" translate="title"></resource>
                            <resource id="Mailjet_Mailjet::config_automation" title="Mailjet Automation Section" translate="title"></resource>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
