<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="newsletter_subscriber_save_commit_after">
        <observer name="mailjet_newsletter_subscriber_save_commit_after" instance="Mailjet\Mailjet\Observer\Newsletter\Subscriber\SaveCommitAfter" />
    </event>
    <event name="newsletter_subscriber_delete_commit_after">
        <observer name="mailjet_newsletter_subscriber_delete_commit_after" instance="Mailjet\Mailjet\Observer\Newsletter\Subscriber\DeleteCommitAfter" />
    </event>
    <event name="customer_delete_commit_after">
        <observer name="mailjet_customer_delete_commit_after" instance="Mailjet\Mailjet\Observer\Customer\DeleteCommitAfter" />
    </event>
    <event name="sales_order_save_after">
        <observer name="mailjet_sales_order_save_after" instance="Mailjet\Mailjet\Observer\Order\SaveAfter" />
    </event>
    <event name="sales_model_service_quote_submit_success">
        <observer name="mailjet_sales_model_service_quote_submit_success" instance="Mailjet\Mailjet\Observer\Checkout\QuoteSubmitSuccess" />
    </event>
</config>
